<?xml version="1.0" encoding="UTF-8"?>
<resources>
    <declare-styleable name="multi_line_radio_group">
        <attr name="max_in_row" format="integer" />
        <attr name="radio_buttons" format="reference" />
        <attr name="default_button" format="string" />
    </declare-styleable>
    <declare-styleable name="CalendarView">
        <attr name="dateFormat" format="string" />
    </declare-styleable>

    <declare-styleable name="SearchableSpinner">
        <attr name="hintText" format="string"/>
    </declare-styleable>

    <attr name="textFieldStyle" format="reference" />
